{
    "name": "graham-campbell/markdown",
    "description": "Markdown Is A Simple PHP Markdown Wrapper For Laravel 4.1",
    "keywords": ["laravel", "framework", "markdown", "php markdown", "markdown wrapper", "php markdown wrapper", "Markdown", "Laravel Markdown", "Laravel-Markdown", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "symfony/config": "2.4.*",
        "symfony/filesystem": "2.4.*",
        "nazar-pc/php-markdown-next": "2.0.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.3.*"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Markdown\\": "src/",
            "GrahamCampbell\\Tests\\Markdown\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-1.1-wip": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
