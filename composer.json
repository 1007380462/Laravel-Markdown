{
    "name": "graham-campbell/markdown",
    "description": "Markdown Is A Parsedown Extra Wrapper For Laravel 5",
    "keywords": ["laravel", "framework", "markdown", "parsedown", "parsedown extra", "markdown wrapper", "parsedown wrapper","parsedown extra wrapper", "Markdown", "Parsedown", "Parsedown Extra", "Laravel Markdown", "Laravel-Markdown", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "illuminate/support": "~5.0",
        "illuminate/view": "~5.0",
        "erusev/parsedown": "~1.0.1",
        "erusev/parsedown-extra": "~0.2.1"
    },
    "require-dev": {
        "graham-campbell/testbench": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "GrahamCampbell\\Markdown\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrahamCampbell\\Tests\\Markdown\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
