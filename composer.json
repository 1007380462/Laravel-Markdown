{
    "name": "graham-campbell/markdown",
    "description": "Markdown Is A Simple PHP Markdown Wrapper For Laravel 4.1",
    "keywords": ["laravel", "framework", "markdown", "php markdown", "markdown wrapper", "php markdown wrapper", "Markdown", "Laravel Markdown", "Laravel-Markdown", "Graham Campbell", "GrahamCampbell"],
    "license": "Apache License 2.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "michelf/php-markdown": "1.4.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.1.*"
    },
    "autoload": {
        "psr-0": {
            "GrahamCampbell\\Markdown\\Classes": "src/",
            "GrahamCampbell\\Markdown\\Facades": "src/",
            "GrahamCampbell\\Markdown\\Support": "src/",
            "GrahamCampbell\\Markdown\\Tests": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
